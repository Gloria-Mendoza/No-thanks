<Window x:Class="NoThanks.Personalization"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NoThanks"
        xmlns:p = "clr-namespace:NoThanks.Properties"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.PERSONALIZATION_TITLE_WINDOW}" Height="450" Width="800">
    <Window.Background>
        <ImageBrush ImageSource="/Images/5053309.jpg"/>
    </Window.Background>
    <Grid>
        <Button Content="{x:Static p:Resources.GENERAL_BACK_BUTTON}" Margin="0,33,66,0" FontFamily="Arial" FontWeight="Bold" FontSize="16" Click="Button_Click" Height="34" VerticalAlignment="Top" HorizontalAlignment="Right" Width="62"/>

    </Grid>
</Window>
