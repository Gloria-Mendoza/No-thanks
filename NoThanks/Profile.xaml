        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p = "clr-namespace:NoThanks.Properties"
        xmlns:local="clr-namespace:NoThanks"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.PROFILE_TITLE_WINDOW}" Height="450" Width="800">
    <Grid>
        <Image HorizontalAlignment="Left" Height="536" VerticalAlignment="Top" Width="960" Source="/5053309.jpg"/>
        <Button Content="{x:Static p:Resources.GENERAL_BACK_BUTTON}" HorizontalAlignment="Left" Margin="677,10,0,0" VerticalAlignment="Top" Height="27" Width="70" Click="Button_Click" FontFamily="Arial" FontWeight="Bold"/>

    </Grid>
</Window>
